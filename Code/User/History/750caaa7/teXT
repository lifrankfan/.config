"xbindkeys_show" 
  control+shift + q

# Underline and add a colon
"sleep 0.2 && xte 'keydown Control_L' 'key a' 'key u' 'keyup Control_L' 'key End' 'keydown Shift_L' 'key colon' 'keyup Shift_L'"
  Control + semicolon
