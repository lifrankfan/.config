"xbindkeys_show" 
  control+shift + q

# underline and then add colon
"sleep 0.2 && xte 'keydown Control_L' 'keydown a' 'keyup a' 'keydown Control_L' 'keydown u' 'keyup u' 'keyup Control_L' 'keydown Right' 'keyup Right' 'keydown '"
  Control+semicolon

